package chapter06;

public class TeamGroup {

	// 멤버변수
	private TeamMember_05[] member = new TeamMember_05[5];
	
	// 기본생성자

	// 초기화 메서드
	public void init() {
		// 생성자 오버로드 사용
		member[0] = new TeamMember_05("신인철", "남학생");
		member[1] = new TeamMember_05("김관호", "남학생");
		member[2] = new TeamMember_05("문원주", "남학생");
		member[3] = new TeamMember_05("임해균", "남학생");
		member[4] = new TeamMember_05("유연우", "여학생");

	}

}
