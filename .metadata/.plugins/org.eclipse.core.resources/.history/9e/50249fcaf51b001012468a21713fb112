package chapter05;

public class MyLotto_14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	// 중복체크 검수하는 메소드
	public static boolean contains(int[] arr, int num) {

		for (int i : arr) {
			if (i == num) { // 배열방에 있는 숫자 == 방금 받은 숫자
				return true; // 중복숫자 발견
			} // if
		} // for
		return false; // 중복숫자 없음
		
	}// contains

}
